
@{
    Layout = null;
}
<!DOCTYPE html>

<html>
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0,shrink-to-fit=no">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="shortcut icon" href="/Image/bitbug_favicon.ico">
    <title>PlanMateAPS-登陆</title>
    <link href="https://cdn.bootcss.com/twitter-bootstrap/4.3.1/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/Css/login.css" rel="stylesheet" />
</head>

<body>
    <div class="wrapper">
        <div class="container">
            <div class="row cont">
                <div class="col-lg-6 col-md-6 col-sm-0 col-xs-0 Logo">
                    <img src="~/Image/logo-white.png" alt="">
                    <h4 class='title'>您身边更专业的计划专家</h4>
                </div>
                <div class="col-lg-6 col-md-6 col-sm-12 col-xm-12">
                    <p id='loginTitle' class="text-center">用户登陆</p>
                    <div class="form-group ml-md-2 ml-sm-2 ml-xl-2 mr-lg-2 mr-md-2 mr-sm-2 mr-xl-2">
                        <input type="text" name="userName" id="userName" class="form-control mb-2" placeholder="用户名" />
                        <input type="password" name="userPass" id="userPass" class="form-control mb-3" TextMode="Password" placeholder="用户密码" />
                        <div class="row pl-3 pr-3">
                            <div class="col col-7 pl-0">
                                <input type="button" name="BtnLogout" id="BtnLogout" class="btn btn-primary btn-block" style="display:none" value="强制登出" onclick="Logout()" />
                            </div>
                            <div class="col col-5 pr-0">
                                <input type="button" name="BtnCancel" id="BtnCancel" class="btn btn-secondary btn-block" style="display:none" value="取消" onclick="deleteCookie()" />
                            </div>
                            <input type="submit" name="BtnLogin" id="BtnLogin" class="btn btn-primary btn-block" value="登陆" style="display:block" onclick="CheckInput()" />
                        </div>
                    </div>
                    <div class="row" style="position:relative">
                        <div id="LoginMsg" class="col-7 text-danger m-3 text-right" style="right:.2em; position:absolute;font-size:.5em;font-weight:500">
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <script src="https://cdn.bootcss.com/jquery/3.4.1/jquery.min.js"></script>
    <script src="https://cdn.bootcss.com/twitter-bootstrap/4.3.1/js/bootstrap.min.js"></script>
    <script src="~/js/index.js" type="text/javascript"></script>
    <script>
        $.get('/DataCenter/TableToExcel').done(function (data) {
            console.log(data);
        })
    </script>
</body>
</html>